{
    "name": "tankbot",
    "version": "2023.2.7.0",
    "description": "Awesome Newgrounds bot",
    "private": true,
    "exports": "./dist/index.js",
    "type": "module",
    "engines": {
        "node": ">=14.16"
    },
    "scripts": {
        "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix && npx prettier --write ./src",
        "build": "rimraf dist && tsc && tsc-alias -p tsconfig.json",
        "start": "node dist/index.js",
        "deploy": "node dist/deploy.js",
        "bump": "bumpver update",
        "release": "standard-version --skip.bump --tag-prefix=\"\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tankbot/bot.git"
    },
    "keywords": [
        "discord",
        "newgrounds"
    ],
    "author": "Breadlike",
    "license": "AGPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/tankbot/bot/issues"
    },
    "homepage": "https://github.com/tankbot/bot#readme",
    "devDependencies": {
        "@types/humanize-duration": "^3.27.1",
        "@types/node": "^18.13.0",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "prettier": "2.8.3",
        "standard-version": "^9.5.0"
    },
    "dependencies": {
        "@discordjs/opus": "^0.9.0",
        "@discordjs/rest": "^1.5.0",
        "@discordjs/voice": "^0.14.0",
        "@distube/yt-dlp": "^1.1.3",
        "@prisma/client": "^4.9.0",
        "discord-api-types": "^0.37.31",
        "discord.js": "^14.7.1",
        "distube": "^4.0.4",
        "dotenv": "^16.0.3",
        "humanize-duration": "^3.28.0",
        "libsodium-wrappers": "^0.7.10",
        "prisma": "^4.9.0",
        "rimraf": "^4.1.2",
        "segfault-handler": "^1.3.0",
        "tsc-alias": "^1.8.2",
        "typescript": "^4.9.5"
    }
}
