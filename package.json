{
    "name": "d-bot",
    "version": "2022.9.6",
    "description": "Awesome Newgrounds bot",
    "private": true,
    "exports": "./dist/index.js",
    "type": "module",
    "engines": {
        "node": ">=14.16"
    },
    "scripts": {
        "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix && npx prettier --write ./src",
        "build": "rimraf dist && tsc && tsc-alias -p tsconfig.json",
        "start": "node dist/index.js",
        "deploy": "node dist/deploy.js",
        "bump": "bumpver update",
        "release": "standard-version --skip.bump"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Featyre/D-Bot.git"
    },
    "keywords": [
        "discord",
        "newgrounds"
    ],
    "author": "Featyre",
    "license": "AGPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/Featyre/D-Bot/issues"
    },
    "homepage": "https://github.com/Featyre/D-Bot#readme",
    "devDependencies": {
        "@types/humanize-duration": "^3.27.1",
        "@types/node": "^18.7.15",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-node": "^11.1.0",
        "prettier": "2.7.1",
        "prisma": "^4.3.1",
        "rimraf": "^3.0.2",
        "standard-version": "^9.5.0",
        "tsc-alias": "^1.7.0",
        "typescript": "^4.8.2"
    },
    "dependencies": {
        "@discordjs/opus": "^0.8.0",
        "@discordjs/rest": "^1.1.0",
        "@discordjs/voice": "^0.11.0",
        "@distube/yt-dlp": "^1.1.3",
        "@prisma/client": "^4.3.1",
        "discord-api-types": "^0.37.5",
        "discord.js": "^14.3.0",
        "distube": "^4.0.3",
        "dotenv": "^16.0.2",
        "humanize-duration": "^3.27.3",
        "libsodium-wrappers": "^0.7.10",
        "phin": "^3.6.1",
        "segfault-handler": "^1.3.0"
    }
}
